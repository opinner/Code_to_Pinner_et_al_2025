# use libertine in matplotlib to match thesis
# https://stackoverflow.com/questions/64405910/matplotlib-font-common-font-with-latex
# text.usetex: True
# text.latex.preamble: \usepackage{libertine}\usepackage[libertine]{newtxmath}\usepackage{amsmath}\usepackage{amssymb}
# font.family: sans-serif

# Use 12pt font in plots, to match 12pt font in document
font.size: 12
axes.labelsize: 12

# Make the legend/label fonts a little smaller
legend.fontsize: 10
xtick.labelsize: 10
ytick.labelsize: 10